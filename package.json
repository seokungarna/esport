{
  "name": "@stackrole/gatsby-starter-foundation",
  "description": "A starter to launch your blazing fast personal website and a blog, Built with Gatsby and Netlify CMS. Made with ‚ù§ by Stackrole",
  "version": "1.0.0",
  "author": "Stackrole <stackrole@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackrole/gatsby-starter-foundation.git"
  },
  "bugs": {
    "url": "https://github.com/stackrole/gatsby-starter-foundation/issues"
  },
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby build ",
    "develop": "set NODE_OPTIONS=--openssl-legacy-provider && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.32.13",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-advanced-sitemap": "^1.5.4",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-sharp": "^2.14.0",
    "gatsby-remark-images": "^3.11.1",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-remark-responsive-iframe": "^2.11.0",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-remark": "^2.16.1",
    "gatsby-transformer-sharp": "^2.12.1",
    "node-sass": "^8.0.0",
    "prismjs": "^1.20.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "acorn": "^6.4.1",
    "prettier": "2.0.5",
    "react-refresh": "^0.8.2",
    "redux": "^3.0.0",
    "typescript": "^3.9.2"
  }
}
